{
  "name": "booking-journey-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Full-stack booking journey application with microservices architecture",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "scripts": {
    "build": "nx run-many --target=build --all",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset --force && npm run db:seed",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "dev": "concurrently \"npm run dev:api-gateway\" \"npm run dev:search\" \"npm run dev:availability\" \"npm run dev:content\" \"npm run dev:order\" \"npm run dev:customer\" \"npm run dev:frontend\"",
    "dev:api-gateway": "cd apps/api-gateway && npm run start:dev",
    "dev:availability": "cd apps/availability-service && npm run start:dev",
    "dev:content": "cd apps/content-service && npm run start:dev",
    "dev:customer": "cd apps/customer-service && npm run start:dev",
    "dev:frontend": "cd apps/frontend && npm run dev",
    "dev:order": "cd apps/order-service && npm run start:dev",
    "dev:search": "cd apps/search-service && npm run start:dev",
    "docker:build": "docker-compose build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:up": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "nx run-many --target=lint --all",
    "seed:db": "docker-compose exec postgres psql -U postgres -d booking_journey -f /docker-entrypoint-initdb.d/01-seed-data.sql",
    "serve:api-gateway": "nx serve api-gateway",
    "serve:availability-service": "nx serve availability-service",
    "serve:content-service": "nx serve content-service",
    "serve:customer-service": "nx serve customer-service",
    "serve:frontend": "nx serve frontend",
    "serve:order-service": "nx serve order-service",
    "serve:search-service": "nx serve search-service",
    "test": "nx run-many --target=test --all",
    "test:e2e": "cd apps/frontend && npm run test:e2e",
    "test:e2e:ui": "cd apps/frontend && npm run test:e2e:ui",
    "type-check": "nx run-many --target=type-check --all"
  },
